# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid "On April 8, Patriot Paul Revere rode to Concord and notified local militias in the area to be on the alert for the British army forces.$^1$"
msgstr ""

