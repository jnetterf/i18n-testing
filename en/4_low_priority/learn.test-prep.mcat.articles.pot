# (KA_SOURCE_TREE: lol)
# (KA_SOURCE_TREE: pt)
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"[[☃ image 13]]\n"
"[[☃ image 14]]"
msgstr ""

