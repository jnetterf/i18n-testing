# Exercise at <a href="http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx">http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx</a>.
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"$\\begin{align}\n"
"&\\phantom{=}\\dfrac{d}{dx}[-4e^x-\\sin(x)]\n"
"\\\\\\\\\n"
"&=-4\\dfrac{d}{dx}[e^x]-\\dfrac{d}{dx}[\\sin(x)]\n"
"\\\\\\\\\n"
"&=-4\\cdot e^x-\\dfrac{d}{dx}[\\sin(x)]\\quad\\gray{\\dfrac{d}{dx}[e^x]=e^x}\n"
"\\\\\\\\\n"
"&=-4e^x-\\cos(x)\\quad\\gray{\\dfrac{d}{dx}[\\sin(x)]=\\cos(x)}\n"
"\\end{align}$"
msgstr ""

# Exercise at <a href="http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx">http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx</a>.
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"$\\begin{align}\n"
"&\\phantom{=}\\dfrac{d}{dx}[-\\ln(x)+x^2]\n"
"\\\\\\\\\n"
"&=-1\\dfrac{d}{dx}[\\ln(x)]+\\dfrac{d}{dx}[x^2]\n"
"\\\\\\\\\n"
"&=-1\\cdot\\dfrac1x+\\dfrac{d}{dx}[x^2]\\quad\\gray{\\dfrac{d}{dx}[\\ln(x)]=\\dfrac1x}\n"
"\\\\\\\\\n"
"&=-\\dfrac1x+(2x^1)\\quad\\gray{\\dfrac{d}{dx}[x^n]=nx^{n-1}}\n"
"\\\\\\\\\n"
"&=-\\dfrac1x+2x\n"
"\\end{align}$"
msgstr ""

# Exercise at <a href="http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx">http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx</a>.
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"$\\begin{align}\n"
"&\\phantom{=}\\dfrac{d}{dx}[2\\cos(x)-3\\ln(x)]\n"
"\\\\\\\\\n"
"&=2\\dfrac{d}{dx}[\\cos(x)]-3\\dfrac{d}{dx}[\\ln(x)]\n"
"\\\\\\\\\n"
"&=2\\dfrac{d}{dx}[\\cos(x)]-3\\cdot\\dfrac1x\\quad\\gray{\\dfrac{d}{dx}[\\ln(x)]=\\dfrac1x}\n"
"\\\\\\\\\n"
"&=2[-\\sin(x)]-\\dfrac3x\\quad\\gray{\\dfrac{d}{dx}[\\cos(x)]=-\\sin(x)}\n"
"\\\\\\\\\n"
"&=-2\\sin(x)-\\dfrac3x\n"
"\\end{align}$"
msgstr ""

# Exercise at <a href="http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx">http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx</a>.
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"$\\begin{align}\n"
"&\\phantom{=}\\dfrac{d}{dx}[3\\ln(x)-12\\sin(x)]\n"
"\\\\\\\\\n"
"&=3\\dfrac{d}{dx}[\\ln(x)]-12\\dfrac{d}{dx}[\\sin(x)]\n"
"\\\\\\\\\n"
"&=3\\cdot\\dfrac1x-12\\dfrac{d}{dx}[\\sin(x)]\\quad\\gray{\\dfrac{d}{dx}[\\ln(x)]=\\dfrac1x}\n"
"\\\\\\\\\n"
"&=\\dfrac3x-12\\cdot\\cos(x)\\quad\\gray{\\dfrac{d}{dx}[\\sin(x)]=\\cos(x)}\n"
"\\end{align}$"
msgstr ""

# Exercise at <a href="http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx">http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx</a>.
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"$\\begin{align}\n"
"&\\phantom{=}\\dfrac{d}{dx}[3e^x+5x^2]\n"
"\\\\\\\\\n"
"&=3\\dfrac{d}{dx}[e^x]+5\\dfrac{d}{dx}[x^2]\n"
"\\\\\\\\\n"
"&=3\\cdot e^x+5\\dfrac{d}{dx}[x^2]\\quad\\gray{\\dfrac{d}{dx}[e^x]=e^x}\n"
"\\\\\\\\\n"
"&=3e^x+5(2x^1)\\quad\\gray{\\dfrac{d}{dx}[x^n]=nx^{n-1}}\n"
"\\\\\\\\\n"
"&=3e^x+10x\n"
"\\end{align}$"
msgstr ""

# Exercise at <a href="http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx">http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx</a>.
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"$\\begin{align}\n"
"&\\phantom{=}\\dfrac{d}{dx}[7\\cos(x)+3e^x]\n"
"\\\\\\\\\n"
"&=7\\dfrac{d}{dx}[\\cos(x)]+3\\dfrac{d}{dx}[e^x]\n"
"\\\\\\\\\n"
"&=7\\dfrac{d}{dx}[\\cos(x)]+3\\cdot e^x\\quad\\gray{\\dfrac{d}{dx}[e^x]=e^x}\n"
"\\\\\\\\\n"
"&=7[-\\sin(x)]+3e^x\\quad\\gray{\\dfrac{d}{dx}[\\cos(x)]=-\\sin(x)}\n"
"\\\\\\\\\n"
"&=-7\\sin(x)+3e^x\n"
"\\end{align}$"
msgstr ""

# Exercise at <a href="http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx">http://translate.khanacademy.org/math/old-ap-calc-bc/bc-differentiation-1-new/bc-3-7/e/derivatives-of-ex-and-lnx</a>.
# (KA_SOURCE_TREE: shared)
# (format: perseus_text)
#: intl/datastore:1
msgid ""
"$\\begin{align}\n"
"f'(x)&=\\dfrac{d}{dx}[-4\\sin(x)-9\\ln(x)]\n"
"\\\\\\\\\n"
"&=-4\\dfrac{d}{dx}[\\sin(x)]-9\\dfrac{d}{dx}[\\ln(x)]\n"
"\\\\\\\\\n"
"&=-4\\dfrac{d}{dx}[\\sin(x)]-9\\cdot\\dfrac1x\\quad\\gray{\\dfrac{d}{dx}[\\ln(x)]=\\dfrac1x}\n"
"\\\\\\\\\n"
"&=-4\\cdot\\cos(x)-\\dfrac9x\\quad\\gray{\\dfrac{d}{dx}[\\cos(x)]=-\\sin(x)}\n"
