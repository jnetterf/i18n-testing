"$\\begin{align}\n"
"&\\phantom{=}\\displaystyle\\int_0^5 xe^{-x}\\,dx \n"
"\\\\\\\\\n"
"&=\\displaystyle\\int_0^5 u\\,dv\n"
"\\\\\\\\\n"
"&=\\Big[uv\\Big]_0^5-\\displaystyle\\int_0^5 v\\,du\n"
"\\\\\\\\\n"
"&=\\displaystyle\\Big[ -xe^{-x}\\Big]_0^5-\\int_0^5-e^{-x}\\,dx\n"
"\\\\\\\\\n"
"&=\\Big[-xe^{-x}-e^{-x}\\Big]_0^5\n"
"\\\\\\\\\n"
"&=\\Big[-e^{-x}(x+1)\\Big]_0^5\n"
"\\\\\\\\\n"
"&=-e^{-5}(6)+e^0(1)\n"
"\\\\\\\\\n"
"&=-6e^{-5}+1\n"
"\\end{align}$"
msgstr ""

